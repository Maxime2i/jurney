{
    "manifest_version": 3,
    "name": "Jurney",
    "version": "1.0",
    "permissions": ["tabCapture", "activeTab", "scripting", "downloads", "storage", "runtime", "debugger"],
    
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  